<?xml version="1.0" encoding="utf-8"?>
<camera_rig coord_axes="y-up" rig_diameter_cm="5.0">
	
    <camera width="2880" height="2880" layout="equatorial">
        <input_calib_file name="Camera2.jpg"/>
        <pose>
            <rotation yaw_deg ="180.0" pitch_deg ="0.0" roll_deg="0.0" />
            <translation x_cm="0.0" y_cm="0.0" z_cm="0.0"/>
        </pose>
        <optics>
            <focal_length focal_pixels="612"/>
            <principal_point center_offset_x="0.0" center_offset_y="0.0"/>
            <lens type="fisheye" k1="0.3854" k2="-0.3000" k3="0.1150" k4="-0.0164"/>
            <fisheye_radius radius_pixels="2035.0"/>
        </optics>
    </camera>
    <camera width="2880" height="2880" layout="equatorial">
        <input_calib_file name="Camera1.jpg"/>
        <pose>
            <rotation yaw_deg="0.0" pitch_deg="0.0" roll_deg="0.0" />
