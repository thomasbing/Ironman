<?xml version="1.0" encoding="utf-8"?>
<!-- The rotation can be specified with the m*, *_deg, or *_rad.
If several specifications are supplied, the yaw-pitch-roll will take precedence. -->
<rig coord_axes="y-up" rig_diameter_cm="21.5">
    <camera width="3680" height="2428" layout="equatorial">
        <input_calib_file name="Camera8.jpg"/>
        <pose>
            <rotation yaw_deg ="45" pitch_deg ="0.0" roll_deg="-90.0" />
            <translation x_cm="0.0" y_cm="0.0" z_cm="0.0"/>
        </pose>
        <optics>
            <focal_length focal_pixels="1095"/>
            <principal_point center_offset_x="0.0" center_offset_y="0.0"/>
            <lens type="fisheye" k1="0.0" k2="0.0" k3="0.0" k4="0.0"/>
            <fisheye_radius radius_pixels="1780.0"/>
        </optics>
        <input_media_feed>
            <input_media_form form="file" />
            <input_media_file name="Camera8.jpg"  fps="30" width="3680" height="2428"/>
        </input_media_feed>
    </camera>
