        </pose>
        <optics>
            <focal_length focal_pixels="612"/>
            <principal_point center_offset_x="0.0" center_offset_y="0.0"/>
            <lens type="fisheye" k1="0.3854" k2="-0.3000" k3="0.1150" k4="-0.0164"/>
            <fisheye_radius radius_pixels="2035.0"/>
        </optics>
    </camera>
</camera_rig>